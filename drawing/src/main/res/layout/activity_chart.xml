<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <com.turastory.drawing.radar.Radar6Chart
    android:layout_width="0dp"
    android:layout_height="0dp"
    android:background="#eee"
    app:layout_constraintDimensionRatio="9:7"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:textSize="14sp"
    app:textColor="#333333"
    app:dotColor="#f7594d"
    app:polygonForegroundDrawable="@drawable/bg_polygon"
    app:polygonBackgroundColor="#1a4a7ce0"
    app:polygonForegroundColor="#3d91e0"/>

</android.support.constraint.ConstraintLayout>